{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*.d.ts",
    "src/renderer/src/**/*.vue",
    "src/preload/*.d.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
        "@main/*": ["src/main/*"],
      "@renderer/*": [
        "src/renderer/src/*"
      ],
        "@preload/*": [
            "src/preload/*"
        ],
        
    }
  }
}
