{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": ["electron.vite.config.*", "src/main/*","src/main/createWindow/*", "src/preload/*"],
  "compilerOptions": {
    "composite": true,
    "types": ["electron-vite/node"],
    "baseUrl": "./",
    "paths": {
      "@main/*": ["src/main/*"],
      "@preload/*": ["src/preload/*"]
    }
  }
}
